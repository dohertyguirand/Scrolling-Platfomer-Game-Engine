<Game>
    <Name>VVVVVV</Name>
    <Thumbnail>VVVVVV-thumbnail.jpg</Thumbnail>
    <Description>A 2010 puzzle-platform video game created by Terry Cavanagh.</Description>
    <Entities>
        <ImageEntity>
            <Name>Avatar</Name>
            <Width>50</Width>
            <Height>100</Height>
            <Image>avatarUp.jpg</Image>
            <VariableNames>Up Down GravityDown RespawnXLocation RespawnYLocation</VariableNames>
            <VariableValues>VVVVVV/avatarUp.jpg VVVVVV/avatarDown.jpg -1 100 500</VariableValues>
            <Behaviors>
                <Behavior>
                    <EntityVariableCondition>
                        <VariableName>GravityDown</VariableName>
                        <RequiredValue>-1</RequiredValue>
                    </EntityVariableCondition>
                    <IndependentAction>
                        <Effect>Gravity 0 1 + 0.1</Effect>
                    </IndependentAction>
                </Behavior>
                <Behavior>
                    <EntityVariableCondition>
                        <VariableName>GravityDown</VariableName>
                        <RequiredValue>1</RequiredValue>
                    </EntityVariableCondition>
                    <IndependentAction>
                        <Effect>Gravity 0 -1 + 0.1</Effect>
                    </IndependentAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>universalTerrain</Entity2>
                        <Direction>Right</Direction>
                    </RequiredCollisionCondition>
                    <CollisionDeterminedAction>
                        <Args>universalTerrain Right</Args>
                        <Effect>TerrainCollisionRight</Effect>
                    </CollisionDeterminedAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>universalTerrain</Entity2>
                        <Direction>Left</Direction>
                    </RequiredCollisionCondition>
                    <CollisionDeterminedAction>
                        <Args>universalTerrain Left</Args>
                        <Effect>TerrainCollisionLeft</Effect>
                    </CollisionDeterminedAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>universalTerrain</Entity2>
                        <Direction>Down</Direction>
                    </RequiredCollisionCondition>
                    <CollisionDeterminedAction>
                        <Args>universalTerrain Down</Args>
                        <Effect>TerrainCollisionDown</Effect>
                    </CollisionDeterminedAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>universalTerrain</Entity2>
                        <Direction>Up</Direction>
                    </RequiredCollisionCondition>
                    <CollisionDeterminedAction>
                        <Args>universalTerrain Up</Args>
                        <Effect>TerrainCollisionUp</Effect>
                    </CollisionDeterminedAction>
                </Behavior>
                <Behavior>
                    <InputCondition>
                        <Key>RightKey</Key>
                        <InputRequirement>true</InputRequirement>
                    </InputCondition>
                    <IndependentAction>
                        <Effect>VelocityInput .100 0 + .3200</Effect>
                    </IndependentAction>
                </Behavior>
                <Behavior>
                    <InputCondition>
                        <Key>LeftKey</Key>
                        <InputRequirement>true</InputRequirement>
                    </InputCondition>
                    <IndependentAction>
                        <Effect>VelocityInput -.100 0 + .3200</Effect>
                    </IndependentAction>
                </Behavior>
                  <Behavior>
                        <RequiredCollisionCondition>
                            <Entity1>Avatar</Entity1>
                            <Entity2>universalTerrain</Entity2>
                            <Direction>ANY</Direction>
                        </RequiredCollisionCondition>
                        <InputCondition>
                          <Key>UpKey</Key>
                          <InputRequirement>Pressed</InputRequirement>
                        </InputCondition>
                        <IndependentAction>
                          <Effect>SetVariable GravityDown 1</Effect>
                              <Effect>SetImage Down</Effect>
                        </IndependentAction>
                  </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>universalTerrain</Entity2>
                        <Direction>ANY</Direction>
                    </RequiredCollisionCondition>
                    <InputCondition>
                        <Key>DownKey</Key>
                        <InputRequirement>Pressed</InputRequirement>
                    </InputCondition>
                    <IndependentAction>
                        <Effect>SetVariable GravityDown -1</Effect>
                        <Effect>SetImage Up</Effect>
                    </IndependentAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>Spikes</Entity2>
                        <Direction>ANY</Direction>
                    </RequiredCollisionCondition>
                    <IndependentAction>
                        <Effect>Respawn RespawnYLocation RespawnXLocation</Effect>
                    </IndependentAction>
                </Behavior>
            </Behaviors>
        </ImageEntity>
        <ImageEntity>
            <Name>Floor</Name>
            <Width>100</Width>
            <Height>50</Height>
            <Image>brick.jpg</Image>
            <VariableNames>TerrainID</VariableNames>
            <VariableValues>universalTerrain</VariableValues>
        </ImageEntity>
        <ImageEntity>
            <Name>Spikes</Name>
            <Width>100</Width>
            <Height>50</Height>
            <Image>spikes.jpg</Image>
        </ImageEntity>
        <ImageEntity>
            <Name>Background</Name>
            <Width>1000</Width>
            <Height>800</Height>
            <Image>background.jpg</Image>
        </ImageEntity>
        <ImageEntity>
            <Name>CheckPoint</Name>
            <Width>50</Width>
            <Height>100</Height>
            <Image>checkpoint.jpg</Image>
            <VariableNames>CheckPointXLocation CheckPointYLocation</VariableNames>
            <VariableValues>400 500</VariableValues>
            <Behaviors>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>CheckPoint</Entity2>
                        <Direction>ANY</Direction>
                        <IndependantAction>
                            <Effect>SetVariable RespawnYLocation CheckPointYLocation</Effect>
                            <Effect>SetVariable RespawnXLocation CheckPointXLocation</Effect>
                        </IndependantAction>
                    </RequiredCollisionCondition>
                </Behavior>
            </Behaviors>
        </ImageEntity>
    </Entities>
