<Game>
    <Name>VVVVVV</Name>
    <Thumbnail>VVVVVV-thumbnail.jpg</Thumbnail>
    <Description>A 2010 puzzle-platform video game created by Terry Cavanagh.</Description>
    <Entities>
        <ImageEntity>
            <Name>Avatar</Name>
            <Width>50</Width>
            <Height>100</Height>
            <Image>avatarUp.jpg</Image>
            <VariableNames>Up Down</VariableNames>
            <VariableValues>avatarUp.jpg avatarDown.jpg</VariableValues>
            <Behaviors>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>Floor</Entity2>
                        <Direction>Up</Direction>
                    </RequiredCollisionCondition>
                    <InputCondition>
                        <Key>DownKey</Key>
                        <InputRequirement>true</InputRequirement>
                    </InputCondition>
                    <IndependentAction>
                        <Effect>Gravity 0 0.1 + 1</Effect>
                        <Effect>SetImage Down</Effect>
                    </IndependentAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>Floor</Entity2>
                        <Direction>Down</Direction>
                    </RequiredCollisionCondition>
                    <InputCondition>
                        <Key>UpKey</Key>
                        <InputRequirement>true</InputRequirement>
                    </InputCondition>
                    <IndependentAction>
                        <Effect>Gravity 0 -0.1 - 1</Effect>
                        <Effect>SetImage Up</Effect>
                    </IndependentAction>
                </Behavior>
                <Behavior>
                    <RequiredCollisionCondition>
                        <Entity1>Avatar</Entity1>
                        <Entity2>Spikes</Entity2>
                        <Direction>ANY</Direction>
                    </RequiredCollisionCondition>
                    <IndependentAction>
                        <Effect>Respawn 150 500</Effect>
                        <Effect>ChangeVariable Lives - 1.0</Effect>
                    </IndependentAction>
                </Behavior>
                <IndependentAction>
                    <Effect>VelocityInputEffect .1 0 0.1</Effect>
                </IndependentAction>
            </Behaviors>
        </ImageEntity>
        <ImageEntity>
            <Name>Floor</Name>
            <Width>100</Width>
            <Height>50</Height>
            <Image>brick.jpg</Image>
        </ImageEntity>
        <ImageEntity>
            <Name>Spikes</Name>
            <Width>100</Width>
            <Height>50</Height>
            <Image>spikes.jpg</Image>
        </ImageEntity>
