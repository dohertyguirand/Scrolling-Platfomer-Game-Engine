<Game>
  <Name>Super Mario Bros</Name>
  <Thumbnail>mario_logo.jpg</Thumbnail>
  <Description>Straight from the mind of Shigeru Miyamoto.</Description>
  <Entities>
    <ImageEntity>
      <Name>SmallMario</Name>
      <Width>80</Width>
      <Height>120</Height>
      <Image>mario_facing_right.png</Image>
      <Behaviors>
        <Behavior>
          <InputCondition>
            <Key>LeftKey2</Key>
            <InputRequirement>true</InputRequirement>
          </InputCondition>
          <NonCollisionEffect>VelocityInput -.1600 0 .3200</NonCollisionEffect>
          <NonCollisionEffect>SetImage example-mario/mario_facing_left.png</NonCollisionEffect>
        </Behavior>
        <Behavior>
          <InputCondition>
            <Key>RightKey2</Key>
            <InputRequirement>true</InputRequirement>
          </InputCondition>
          <NonCollisionEffect>VelocityInput .1600 0 .3200</NonCollisionEffect>
          <NonCollisionEffect>SetImage example-mario/mario_facing_right.png</NonCollisionEffect>
        </Behavior>
        <Behavior>
          <HorizontalCollisionCondition>
            <EntityName>Koopa</EntityName>
            <CollisionRequirement>true</CollisionRequirement>
          </HorizontalCollisionCondition>
          <CollisionEffect>
            <With>Koopa</With>
            <Reaction>Respawn 0.0 0.0</Reaction>
            <Reaction>ChangeVariable Lives -1.0</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <VerticalCollisionCondition>
            <EntityName>Koopa</EntityName>
            <CollisionRequirement>true</CollisionRequirement>
          </VerticalCollisionCondition>
          <CollisionEffect>
            <With>Koopa</With>
            <Reaction>VerticalBounce -.50</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <VerticalCollisionCondition>
            <EntityName>Brick</EntityName>
            <CollisionRequirement>true</CollisionRequirement>
          </VerticalCollisionCondition>
          <CollisionEffect>
            <With>Brick</With>
            <Reaction>TerrainCollision</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <HorizontalCollisionCondition>
            <EntityName>Brick</EntityName>
            <CollisionRequirement>true</CollisionRequirement>
          </HorizontalCollisionCondition>
          <CollisionEffect>
            <With>Brick</With>
            <Reaction>TerrainCollision</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <CollisionEffect>
            <With>Flagpole</With>
            <Reaction>TerrainCollision</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <CollisionEffect>
            <With>QuestionBlock</With>
            <Reaction>TerrainCollision</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <CollisionEffect>
            <With>Void</With>
            <Reaction>ChangeVariable Lives -1.0</Reaction>
            <Reaction>Respawn 0 0</Reaction>
          </CollisionEffect>
        </Behavior>
        <Behavior>
          <NonCollisionEffect>Gravity 0 0.1</NonCollisionEffect>
        </Behavior>
        <Behavior>
          <InputCondition>
            <Key>UpKey2Pressed</Key>
            <InputRequirement>true</InputRequirement>
          </InputCondition>
          <VerticalCollisionCondition>
            <EntityName>Brick</EntityName>
            <CollisionRequirement>true</CollisionRequirement>
          </VerticalCollisionCondition>
          <NonCollisionEffect>Jump -1.2</NonCollisionEffect>
        </Behavior>
      </Behaviors>
    </ImageEntity>
    <ImageEntity>
      <Name>Koopa</Name>
      <Width>100</Width>
      <Height>120</Height>
      <Image>koopa.png</Image>
      <Behavior>
        <NonCollisionEffect>Gravity 0.0 0.10</NonCollisionEffect>
