<Game>
  <Name>Super Mario Bros</Name>
  <Thumbnail>mario_logo.jpg</Thumbnail>
  <Description>Straight from the mind of Shigeru Miyamoto.</Description>
  <Entities>
    <ImageEntity>
      <Name>SmallMario</Name>
      <Width>80</Width>
      <Height>120</Height>
      <Image>mario.png</Image>
      <Movement>
        <MovementBehavior>Gravity 0 0.1</MovementBehavior>
      </Movement>
      <ConditionalBehaviors>
        <ConditionalInputBehavior>
          <VerticalCollisionCondition>
            <EntityName>Brick</EntityName>
            <CollisionRequirement>true</CollisionRequirement>
          </VerticalCollisionCondition>
          <Key>UpKey2Pressed</Key>
          <ControlBehavior>JumpBehavior -1.2</ControlBehavior>
        </ConditionalInputBehavior>
      </ConditionalBehaviors>
      <Controls>
<!--        <ControlInput>-->
<!--          <Key>UpKey2Pressed</Key>-->
<!--          <ControlBehavior>JumpBehavior -1.2</ControlBehavior>-->
<!--        </ControlInput>-->
<!--        <ControlInput>-->
<!--          <Key>DownKey2</Key>-->
<!--          <ControlBehavior>VelocityInput 0 .1600 .3200</ControlBehavior>-->
<!--        </ControlInput>-->
        <ControlInput>
          <Key>LeftKey2</Key>
          <ControlBehavior>VelocityInput -.1600 0 .3200</ControlBehavior>
        </ControlInput>
        <ControlInput>
          <Key>RightKey2</Key>
          <ControlBehavior>VelocityInput .1600 0 .3200</ControlBehavior>
        </ControlInput>
      </Controls>
      <Collisions>
        <EntityCollision>
          <With>Koopa</With>
          <Reaction>VerticalBounce -.50 </Reaction>
          <Reaction>RunIntoStompable</Reaction>
          <Reaction>ChangeVariable Lives -1.0</Reaction>
        </EntityCollision>
        <EntityCollision>
          <With>Brick</With>
          <Reaction>TerrainCollision</Reaction>
        </EntityCollision>
        <EntityCollision>
          <With>Flagpole</With>
          <Reaction>TerrainCollision</Reaction>
        </EntityCollision>
        <EntityCollision>
          <With>QuestionBlock</With>
          <Reaction>TerrainCollision</Reaction>
        </EntityCollision>
      </Collisions>
    </ImageEntity>
    <ImageEntity>
      <Name>Koopa</Name>
      <Width>100</Width>
      <Height>120</Height>
      <Image>koopa.png</Image>
      <Movement>
        <MovementBehavior>Gravity 0.0 0.10</MovementBehavior>
        <MovementBehavior>Patrol 0.100 .10 500 485 700 485</MovementBehavior>
      </Movement>
      <Collisions>
        <EntityCollision>
          <With>Brick</With>
          <Reaction>TerrainCollision</Reaction>
        </EntityCollision>
        <EntityCollision>
          <With>SmallMario</With>
          <Reaction>Stompable</Reaction>
        </EntityCollision>
      </Collisions>
    </ImageEntity>
    <ImageEntity>
      <Name>Brick</Name>
      <Width>100</Width>
      <Height>100</Height>
      <Image>mario_brick.png</Image>
    </ImageEntity>
    <ImageEntity>
      <Name>Mushroom</Name>
      <Width>80</Width>
      <Height>80</Height>
      <Image>mario_mushroom.png</Image>
      <Movement>
        <MovementBehavior>MoveForward 0.10 0.0 0.20</MovementBehavior>
        <MovementBehavior>Gravity 0.0 0.10</MovementBehavior>
      </Movement>
      <Collisions>
        <EntityCollision>
        <With>Brick</With>
        <Reaction>TerrainCollision</Reaction>
        </EntityCollision>
        <EntityCollision>
          <With>QuestionBlock</With>
          <Reaction>TerrainCollision</Reaction>
        </EntityCollision>
        <EntityCollision>
          <With>SmallMario</With>
          <Reaction>RemoveSelf</Reaction>
        </EntityCollision>
      </Collisions>
    </ImageEntity>
    <ImageEntity>
      <Name>Flagpole</Name>
      <Width>100</Width>
      <Height>400</Height>
      <Image>mario_flagpole.png</Image>
    </ImageEntity>
    <ImageEntity>
      <Name>QuestionBlock</Name>
      <Width>100</Width>
      <Height>100</Height>
      <Image>mario_question_block.png</Image>
      <Collisions>
        <EntityCollision>
          <With>SmallMario</With>
          <Reaction>HeadHitItem Mushroom</Reaction>
        </EntityCollision>
      </Collisions>
    </ImageEntity>
  </Entities>
